# ============================================
# Train Ticket Notifier - Environment Config
# ============================================
# Copy this file to .env and fill in your values

# Provider: tiketkai atau traveloka
PROVIDER=tiketkai

# Telegram Bot
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CHAT_ID=your_chat_id_here

# Webhook Mode (requires cloudflared installed)
USE_WEBHOOK=false
WEBHOOK_PORT=8080

# Train Configuration
TRAIN_NAME=BENGAWAN
TRAIN_ORIGIN=LPN
TRAIN_DESTINATION=CKR
TRAIN_DATE=2026-02-16
TRAIN_ENABLED=True
TRAIN_INTERVAL=180
TRAIN_MAX_PRICE=300000
# TRAIN_FILTER_CLASS=EKONOMI,BISNIS
